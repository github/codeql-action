    "@octokit/core": "^6.1.5",
    "@octokit/plugin-rest-endpoint-methods": "^14.0.0",
    "@octokit/tsconfig": "^4.0.0",
    "@types/node": "^22.0.0",
    "@vitest/coverage-v8": "^3.0.0",
    "esbuild": "^0.25.0",
    "fetch-mock": "^12.0.0",
    "github-openapi-graphql-query": "^4.0.0",
    "glob": "^11.0.0",
    "npm-run-all2": "^7.0.0",
    "prettier": "3.5.3",
    "semantic-release-plugin-update-version-in-files": "^2.0.0",
